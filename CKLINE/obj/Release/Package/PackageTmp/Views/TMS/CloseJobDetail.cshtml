        for (var i = 1 ; i <= OTHcounter; i++) {
            OTHSumPrice = 'OTHSumPrice' + i;

            if(document.getElementById(OTHSumPrice).value == '')
            {
                OTHSumValue = 0;
            }
            else
            {
                OTHSumValue = parseFloat(document.getElementById(OTHSumPrice).value);
            }

            OTHAllSum = OTHAllSum + OTHSumValue;
           
        }
        
        document.getElementById('OTHAllSum').value = OTHAllSum;
       
    }
    function isNumberKey(evt) {
      
        var charCode = (evt.which) ? evt.which : event.keyCode;
        if (charCode != 46 && charCode > 31
          && (charCode < 48 || charCode > 57))
            return false;
     
        return true;
       
    }
</script>
