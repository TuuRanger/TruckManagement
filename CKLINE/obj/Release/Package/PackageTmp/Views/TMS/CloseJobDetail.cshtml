    function sum() {
        var WCarB = document.getElementById('WCarB').value;
        var WGas = document.getElementById('WGas').value;
        if (document.getElementById('WCarB').value == '')
        {
            WCarB = 0;
        }
        if (document.getElementById('WGas').value == '') {
            WGas = 0;
        }
        var result = parseFloat(WCarB) + parseFloat(WGas);
        if (!isNaN(result)) {
            document.getElementById('WCarW').value = result;
        }
       
    }

    function sumOli() {

        var Olicounter = document.getElementById("Olicounter").value;
        var OliNum = '';
        var OliPrice = '';
        var OliSumPrice = '';
        var OliAllNum = 0;
        var OliAllSum = 0;
        var OliSumValue = 0;
        for (var i = 1 ; i <= Olicounter; i++) {

            OliNum = 'OliNum' + i;
            OliPrice = 'OliPrice' + i;
            OliSumPrice = 'OliSumPrice' + i;

            if (document.getElementById(OliNum).value == '') {
                OliNum = 0;
            }
            else {
                OliNum = parseFloat(document.getElementById(OliNum).value);
            }

            if (document.getElementById(OliPrice).value == '') {
                OliPrice = 0;
            }
            else {
                OliPrice = parseFloat(document.getElementById(OliPrice).value);
            }

            document.getElementById(OliSumPrice).value = OliNum * OliPrice;
            OliAllNum = OliAllNum + OliNum;
            OliAllSum = OliAllSum + (OliNum * OliPrice);

        }

        document.getElementById('OliAllNum').value = OliAllNum;
        document.getElementById('OliAllSum').value = OliAllSum;

    }
    function sumWay() {

        var Waycounter = document.getElementById("Waycounter").value;
        var WaySumPrice = '';
        var WayAllSum = 0;
        var WaySumValue = 0;
        for (var i = 1 ; i <= Waycounter; i++) {
            WaySumPrice = 'WaySumPrice' + i;

            if (document.getElementById(WaySumPrice).value == '') {
                WaySumPrice = 0;
            }
            else {
                WaySumPrice = parseFloat(document.getElementById(WaySumPrice).value);
            }

            WayAllSum = WayAllSum + WaySumPrice;

        }

        document.getElementById('WayAllSum').value = WayAllSum;

    }
    function sumOTH() {

        var OTHcounter = document.getElementById("OTHcounter").value;
        var OTHSumPrice = '';
        var OTHAllSum = 0;
