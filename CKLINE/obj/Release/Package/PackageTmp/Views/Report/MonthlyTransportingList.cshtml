@model IEnumerable<CKLINE.Models.MonthlyTransporting>

<section class="content-header">
    <h1>
        การขนส่งประจำเดือน

    </h1>

</section>
<section class="content">
    <div class="box">
        <div class="box">
            <div class="box-header with-border">
                <h3 class="box-title">ค้นหา</h3>
                <div class="box-tools pull-right">
                    <button class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse"><i class="fa fa-minus"></i></button>
                </div>
            </div>
            <div class="box-body">
                <form class="form-horizontal">
                    <div class="box-body">
                        <div class="form-group">
                            <label for="inputEmail3" class="col-sm-2 control-label">เดือน</label>
                            <div class="col-sm-10">
                                    <select class="form-control select2 select2-hidden-accessible" style="width: 100%;" tabindex="-1" aria-hidden="true">
                                        <option selected="selected">มกราคม</option>
                                        <option>กุมภาพันธ์</option>
                                        <option>มีนาคม</option>
                                        <option>เมษายน</option>
                                        <option>พฤษภาคม</option>
                                        <option>มิถุนายน</option>
                                        <option>กรกฎาคม</option>
                                        <option>สิงหาคม</option>
                                        <option>กันยายน</option>
                                        <option>ตุลาคม</option>
                                        <option>พฤศจิกายน</option>
                                        <option>ธันวาคม</option>
                                    </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputEmail3" class="col-sm-2 control-label">ประเภท</label>
                            <div class="col-sm-10">
                                
                                    <select class="form-control select2 select2-hidden-accessible" style="width: 100%;" tabindex="-1" aria-hidden="true">
                                        <option selected="selected">Praxair</option>
                                        <option>Praxair</option>
                                        <option>Trucking</option>
                                        <option>Import/Export</option>
                                    </select>
                                
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputEmail3" class="col-sm-2 control-label">คำค้น</label>
                            <div class="col-sm-10">
                                <input type="email" class="form-control" id="inputEmail3" placeholder="คำค้น">
                            </div>
                        </div>
                    </div><!-- /.box-body -->
                    <div class="box-footer">
                        <button type="submit" class="btn btn-primary">ค้นหา</button>
                    </div><!-- /.box-footer -->
                </form>
            </div><!-- /.box-body -->

        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">รายละเอียด</h3>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-minus"></i>
                        </button>

                    </div>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <div class="table-responsive">
                        <table class="table no-margin">
                            <thead>
                                <tr>
                                    <th>
                                        เดือน
                                    </th>
                                    <th>
                                        จำนวนวัน
                                    </th>
                                    <th>
                                        ทะเบียนหัว
                                    </th>
                                    <th>
                                        ทะเบียนหาง
                                    </th>
                                    <th>
                                        ระยะทางS
                                    </th>
                                    <th>
                                        ระยะทางA
                                    </th>
                                    <th>
                                        ผลค่าง
                                    </th>
                                    <th>
                                        น้ำมันA
                                    </th>
                                    <th>
                                        น้ำมันS
                                    </th>
                                    <th>
                                        ผลต่าง
                                    </th>
                                    <th>
                                        ค่าเที่ยว
                                    </th>
                                    <th>
                                        ค่าน้ำมัน
                                    </th>
                                    <th>
                                        ค่าปะยาง
                                    </th>
                                    <th>
                                        อื่นๆ
                                    </th>
                                    <th>
                                        ค่าขนส่ง
                                    </th>
                                    <th>
                                        กำไร/ขาดทุน
                                    </th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model != null)
                                {
                                    foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Month)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TotalDate)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Head)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Tail)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.DistanceS)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.DistanceA)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.DiffDistance)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.GasA)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.GasS)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.DiffGas)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.PayPeriodCost)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.GasCost)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Recap)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Other)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TransportCost)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.ProfitLoss)
                                            </td>

                                        </tr>  }
                                }
                            </tbody>
                        </table>
                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.box-body -->
                <div class="box-footer clearfix">
                    <a href="#" class="btn btn-sm btn-info btn-flat pull-right">Export to Excel</a>
                    <a href="@Url.Action("MonthlyTransporting", "Report")" class="btn btn-sm btn-default btn-flat pull-left">View</a>
                </div>
                <!-- /.box-footer -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
</section>